{
  "name": "familycare-help",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "dev:marketing": "turbo dev --filter=@familycare/marketing",
    "dev:dashboard": "turbo dev --filter=@familycare/dashboard",
